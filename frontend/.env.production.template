# Production Environment Variables Template
# Copy this file to .env.production and fill in your actual values

# Backend API URL (replace with your deployed backend URL)
REACT_APP_API_URL=https://your-backend-api.herokuapp.com/api

# WebSocket URL (replace with your deployed backend URL)
REACT_APP_WS_URL=wss://your-backend-api.herokuapp.com

# Environment
REACT_APP_ENV=production

# Build optimizations
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false